{
  "name": "covid-parenting-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --aot --buildOptimizer",
    "build-ng": "ng build --prod --aot --buildOptimizer",
    "build": "npm run convert-drive-assets && npm run build-ng && npm run scully",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "upload": "npm run build && npm run upload-files",
    "upload-files": "gsutil -m rsync -d -R -c ./dist/static gs://covid-website-storage2 && gsutil web set -m index.html gs://covid-website-storage2/ && open-cli http://covidp2.idems.international",
    "golive": "gsutil -m -h \"Cache-Control:public, max-age=600\" rsync -R -c gs://covid-website-storage2 gs://covid-website-storage1 && gsutil web set -m index.html gs://covid-website-storage1/ && open-cli https://covid19parenting.com",
    "deploy-cleanup": "gsutil -m -h \"Cache-Control:public, max-age=600\" rsync -R -d gs://covid-website-storage2 gs://covid-website-storage1 && gsutil web set -m index.html gs://covid-website-storage1/",
    "scully": "scully",
    "scully:serve": "scully serve",
    "convert-drive-assets": "ts-node --project tsconfig.scripts.json scripts/convert-drive-assets.ts",
    "test-scripts": "jasmine-ts --project tsconfig.scripts.json --config=scripts.jasmine.json",
    "upload-tip-sheets": "gsutil -m rsync -R -c ./tip_sheets gs://covid-website-pdfs1/assets/tip_sheets"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/localize": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@ionic/angular": "^5.2.2",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@scullyio/init": "^1.0.0-beta.3",
    "@scullyio/ng-lib": "^1.0.0-beta.2",
    "@scullyio/scully": "^1.0.0-beta.2",
    "aos": "^2.3.4",
    "bootstrap": "^4.4.0",
    "leaflet": "^1.7.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.21",
    "@types/node": "^12.11.1",
    "@types/xlsx": "^0.0.36",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open-cli": "~6.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "xlsx": "^0.16.8"
  }
}
